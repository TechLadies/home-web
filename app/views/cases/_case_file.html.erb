<tr class="case">
  <td><%= link_to case_file.id, case_path(case_file) %> </td>
  <td><%= case_file.case_type %> </td>
  <td><%= case_file.client.try(:involvable).try(:name) %></td>
  <td><%= case_file.employer.try(:involvable).try(:name) %></td>
  <td><%= case_file.user.name %></td>
  <td><%= link_to 'Edit', edit_case_path(case_file), class: 'btn btn-default btn-sm' %></td>
</tr>
