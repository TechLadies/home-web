<div class='page-header'>
  <div class='container'>
    <h1>New Organization</h1>
  </div>
</div>

<div class='container'>
  <div class='row'>
    <div class='col-xs-12'>

      <%= simple_form_for @organization do |f| %>

        <%= f.input :name %>
        <%= f.input :phone %>
        <%= f.input :address %>
        <%= f.input :industry %>

        <%= f.submit 'Create Organization', class: 'btn btn-primary' %>
      <%- end %>

    </div>
  </div>
</div>